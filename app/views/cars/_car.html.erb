<div id="<%= dom_id car %>">
  <p>
    <strong>Modelo:</strong>
    <%= car.model %>
  </p>

  <p>
    <strong>Marca:</strong>
    <%= car.brand %>
  </p>

  <p>
    <strong>Caracteristicas:</strong>
    <%= car.features %>
  </p>

  <p>
    <strong>Año fabricacion:</strong>
    <%= car.fabrication_year %>
  </p>

  <p>
    <strong>Autonomía:</strong>
    <%= car.autonomy %>
  </p>

  <p>
    <strong>Precio</strong>
    <%= car.price %>
  </p>

  <p>
    <strong>Imagen:</strong>
    <%= car.imag%>
  </p>

</div>
